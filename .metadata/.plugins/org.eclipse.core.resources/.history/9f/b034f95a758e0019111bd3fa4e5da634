package genetic;

public abstract class CrossoverRandomIndex extends Crossover {

	private int startIndex = -1;
	private int endIndex = -1;
	
	public CrossoverRandomIndex(CrossoverType type, Chromosome firstParent, Chromosome secondParent,
								int startIndex, int endIndex) {
		super(type, firstParent, secondParent);
		this.startIndex = startIndex;
		this.endIndex = endIndex;
	
	}

	@Override
	public void start() {
		// TODO Auto-generated method stub

	}

}
